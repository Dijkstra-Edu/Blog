{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "virtual:*": ["./node_modules/astro-pure/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
